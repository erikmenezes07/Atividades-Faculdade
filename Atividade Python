temperatura = float(input("Digite a temperatura: "))

if temperatura > 40:
    print("Estamos derretendo!")
elif temperatura > 30:
    print("Calor!")
elif temperatura == 0:
    print("T치 muito frio")
elif temperatura > 0 and temperatura < 20:
    print("Ainda t치 frio")
else:
    print("A temperatura est치 agrad치vel")
